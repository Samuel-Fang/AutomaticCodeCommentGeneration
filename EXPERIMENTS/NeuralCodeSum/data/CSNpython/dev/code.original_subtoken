def measure memory ( cls , obj , seen = None ) : size = sys . getsizeof ( obj ) if seen is None : seen = set ( ) obj id = id ( obj ) if obj id in seen : return 0 seen . add ( obj id ) if isinstance ( obj , dict ) : size += sum ( [ cls . measure memory ( v , seen ) for v in obj . values ( ) ] ) size += sum ( [ cls . measure memory ( k , seen ) for k in obj . keys ( ) ] ) elif hasattr ( obj , ' dict ' ) : size += cls . measure memory ( obj . dict , seen ) elif hasattr ( obj , ' iter ' ) and not isinstance ( obj , ( str , bytes , bytearray ) ) : size += sum ( [ cls . measure memory ( i , seen ) for i in obj ] ) return size
