def generate keys ( self , username , hostname ) : r = self . local renderer #r.env.key filename = r.env.key filename or env.key filename #assert r.env.key filename, 'r.env.key filename or env.key filename must be set. e.g. roles/role/app name-role.pem' r . env . key filename = self . env . key filename template . format ( ROLE = self . genv . ROLE , host = hostname , username = username , ) if os . path . isfile ( r . env . key filename ) : r . pc ( 'Key file {key filename} already exists. Skipping generation.' . format ( * * r . env ) ) else : r . local ( "ssh-keygen -t {key type} -b {key bits} -f {key filename} -N ''" ) r . local ( 'chmod {key perms} {key filename}' ) if r . env . key filename . endswith ( '.pem' ) : src = r . env . key filename + '.pub' dst = ( r . env . key filename + '.pub' ) . replace ( '.pem' , '' ) r . env . src = src r . env . dst = dst r . local ( 'mv {src} {dst}' ) return r . env . key filename
