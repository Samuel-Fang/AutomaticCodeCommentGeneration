def pop data ( self , nbytes ) : last timestamp = 0 data = [ ] for packet in self . pop ( nbytes ) : last timestamp = packet . timestamp data . append ( packet . data . data ) return '' . join ( data ) , last timestamp
def push ( self , ip packet ) : data len = len ( ip packet . data . data ) seq id = ip packet . data . seq if data len == 0 : self . next seq id = seq id return False if self . next seq id != - 1 and seq id != self . next seq id : return False self . next seq id = seq id + data len with self . lock packets : self . length += len ( ip packet . data . data ) self . remaining += len ( ip packet . data . data ) self . packets . append ( ip packet ) return True
