def byte number string ( number , thousands Sep = True , partition = False , base1024 = True , append Bytes = True ) : magsuffix = "" bytesuffix = "" if partition : magnitude = 0 if base1024 : while number >= 1024 : magnitude += 1 number = number >> 10 else : while number >= 1000 : magnitude += 1 number /= 1000.0 magsuffix = [ "" , "K" , "M" , "G" , "T" , "P" ] [ magnitude ] if append Bytes : if number == 1 : bytesuffix = " Byte" else : bytesuffix = " Bytes" if thousands Sep and ( number >= 1000 or magsuffix ) : snum = "{:,d}" . format ( number ) else : snum = str ( number ) return "{}{}{}" . format ( snum , magsuffix , bytesuffix )
