def stream data ( self , environ , content length , block size ) : if content length == 0 : logger . info ( "PUT: Content-Length == 0. Creating empty file..." ) else : assert content length > 0 contentremain = content length while contentremain > 0 : n = min ( contentremain , block size ) readbuffer = environ [ "wsgi.input" ] . read ( n ) if not len ( readbuffer ) > 0 : logger . error ( "input.read({}) returned 0 bytes" . format ( n ) ) break environ [ "wsgidav.some input read" ] = 1 yield readbuffer contentremain -= len ( readbuffer ) if contentremain == 0 : environ [ "wsgidav.all input read" ] = 1
