def type size ( ty ) : if ty [ 0 ] in ( 'int' , 'uint' , 'bytes M' , 'function' ) : return 32 elif ty [ 0 ] in ( 'tuple' ) : result = 0 for ty i in ty [ 1 ] : result += ABI . type size ( ty i ) return result elif ty [ 0 ] in ( 'array' ) : rep = ty [ 1 ] result = 32 return result elif ty [ 0 ] in ( 'bytes' , 'string' ) : result = 32 return result raise Value Error
