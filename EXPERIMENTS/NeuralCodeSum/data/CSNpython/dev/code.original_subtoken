def handle ( self , * args , * * options ) : LOGGER . info ( 'Starting assigning enterprise roles to users!' ) role = options [ 'role' ] if role == ENTERPRISE ADMIN ROLE : self . assign enterprise role to users ( self . get enterprise admin users batch , options ) elif role == ENTERPRISE OPERATOR ROLE : self . assign enterprise role to users ( self . get enterprise operator users batch , options ) elif role == ENTERPRISE LEARNER ROLE : self . assign enterprise role to users ( self . get enterprise customer users batch , options ) elif role == ENTERPRISE ENROLLMENT API ADMIN ROLE : self . assign enterprise role to users ( self . get enterprise enrollment api admin users batch , options , True ) elif role == ENTERPRISE CATALOG ADMIN ROLE : self . assign enterprise role to users ( self . get enterprise catalog admin users batch , options , True ) else : raise Command Error ( 'Please provide a valid role name. Supported roles are {admin} and {learner}' . format ( admin = ENTERPRISE ADMIN ROLE , learner = ENTERPRISE LEARNER ROLE ) ) LOGGER . info ( 'Successfully finished assigning enterprise roles to users!' )
